{
  "extends": ["@react-native-community","airbnb-typescript","prettier","prettier/@typescript-eslint","prettier/react"],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    },
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }]
  }
}
