{
  "expo": {
    "name": "Gearfinder",
    "slug": "gearfinder",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#282e39"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barylo.gearfinder",
      "icon": "./assets/app_icon.png"
    },
    "android": {
      "package": "com.barylo.gearfinder",
      "icon": "./assets/app_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/android_icon.png",
        "backgroundColor": "#292f3a"
      }
    },
    "description": "Snowboard, ski size calculator",
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "Gearfinder",
  "name": "gearfinder"
}